{
  "name": "mattermost-load-test-ng/browser",
  "author": "Mattermost, Inc.",
  "type": "module",
  "engines": {
    "node": "22.16.x",
    "npm": ">=10.9.2"
  },
  "engine-strict": true,
  "devDependencies": {
    "@types/ms": "2.1.0",
    "@types/node": "24.0.3",
    "@types/supertest": "6.0.3",
    "@vitest/ui": "3.2.4",
    "esbuild": "^0.24.0",
    "prettier": "3.6.0",
    "supertest": "7.1.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.1",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@playwright/test": "^1.53.1",
    "fastify": "5.4.0",
    "ms": "2.1.3",
    "playwright": "1.53.1"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:prod",
    "build:clean": "rm -rf dist",
    "build:prod": "NODE_ENV=production node scripts/build.js",
    "build:dev": "NODE_ENV=development node scripts/build.js",
    "build:watch": "NODE_ENV=development node scripts/build.js --watch",
    "start": "node dist/server.js",
    "dev": "npm run build:watch",
    "lint:check": "prettier --check 'src/**/*.ts'",
    "lint:fix": "prettier --write 'src/**/*.ts'",
    "types:check": "tsc --noEmit",
    "e2etest:run": "playwright test",
    "e2etest:ui": "playwright test --ui",
    "e2etest:debug": "playwright test --debug",
    "unittest:run": "vitest run",
    "unittest:ui": "vitest --ui",
    "unittest:watch": "vitest"
  }
}
