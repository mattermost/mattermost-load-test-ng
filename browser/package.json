{
  "name": "mattermost-load-test-ng/browser",
  "author": "Mattermost, Inc.",
  "license": "Apache-2.0",
  "type": "module",
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.0.0",
    "@vitest/ui": "^3.2.3",
    "dotenv": "^16.5.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "fastify": "^5.3.3",
    "ms": "^2.1.3",
    "playwright": "^1.53.0"
  },
  "scripts": {
    "postinstall": "node scripts/post_install.mjs",
    "lint:check": "prettier --check 'src/**/*.ts' 'scripts/**/*.mjs'",
    "lint:fix": "prettier --write 'src/**/*.ts' 'scripts/**/*.mjs'",
    "types:check": "tsc --noEmit",
    "e2etest:run": "playwright test",
    "e2etest:ui": "playwright test --ui",
    "e2etest:debug": "playwright test --debug",
    "unittest:run": "vitest run",
    "unittest:ui": "vitest --ui",
    "unittest:watch": "vitest",
    "server:dev": "tsx src/index.ts",
    "server:watch": "tsx watch src/index.ts",
    "server:build": "tsc",
    "server:start": "node scripts/start_server.mjs",
    "clean": "rm -rf node_modules test-results"
  }
}
